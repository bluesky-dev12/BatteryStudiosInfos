class BTWindowErrorDefineHK extends BTWindowInfoHK
    editinlinenew
    instanced;

var localized string strErrorTitle[2];
var localized string strErrorDefine[1200];

function SetContentText_Instance(int Title, int Code, optional string arg0, optional string arg1, optional string arg2, optional string arg3)
{
    local string str_Title, str_Msg;

    str_Msg = Class'Engine.BTCustomDrawHK'.static.FormatString(strErrorDefine[Code], arg0, arg1, arg2);
    str_Title = strErrorTitle[Title];
    SetData(str_Title, 5, Class'Engine.BTCustomDrawHK'.static.FormatString(str_Msg @ "[%0]", string(Code)));
    // End:0x89
    if(Class'GUIWarfareControls_Decompressed.BTWindowErrorDefineHK'.static.ExceptionCondtion(Code))
    {
        return;
    }
    Controller.ViewportOwner.Actor.Level.GetMatchMaker().SendErrorReportS(Code, str_Msg);
    //return;    
}

static function bool ExceptionCondtion(int ErrorCode)
{
    // End:0x13E
    if(((((((((((((((((((((ErrorCode == 16) || ErrorCode == 32) || ErrorCode == 62) || ErrorCode == 68) || ErrorCode == 73) || ErrorCode == 100) || ErrorCode == 130) || ErrorCode == 134) || ErrorCode == 151) || ErrorCode == 155) || ErrorCode == 205) || ErrorCode == 208) || ErrorCode == 229) || ErrorCode == 495) || ErrorCode == 497) || ErrorCode == 498) || ErrorCode == 499) || ErrorCode == 566) || ErrorCode == 567) || ErrorCode == 579) || ErrorCode == 613)
    {
        return true;
    }
    return false;
    //return;    
}

static function string GetFormatString(int Code, optional string arg0, optional string arg1, optional string arg2)
{
    return Class'Engine.BTCustomDrawHK'.static.FormatString(default.strErrorDefine[Code], arg0, arg1, arg2);
    //return;    
}

static function ShowError(GUIController con, int ErrorCode, optional byte titleID)
{
    local string strTitle, strMsg;

    con.OpenMenu("GUIWarfareControls.BTWindowErrorDefineHK");
    strTitle = BTWindowErrorDefineHK(con.TopPage()).strErrorTitle[int(titleID)];
    strMsg = BTWindowErrorDefineHK(con.TopPage()).strErrorDefine[ErrorCode];
    BTWindowErrorDefineHK(con.TopPage()).SetData(strTitle, 5, Class'Engine.BTCustomDrawHK'.static.FormatString(strMsg @ "[%0]", string(ErrorCode)));
    Log((("[BTWindowErrorDefineHK::ShowError] ErrorCode=" $ string(ErrorCode)) $ " Msg=") $ strMsg);
    // End:0x12F
    if(ExceptionCondtion(ErrorCode))
    {
        return;
    }
    con.ViewportOwner.Actor.Level.GetMatchMaker().SendErrorReportS(ErrorCode, strMsg);
    //return;    
}

static function ShowErrorMsg(GUIController con, string ErrorMsg, optional byte titleID)
{
    local string strTitle, strMsg;

    con.OpenMenu("GUIWarfareControls.BTWindowErrorDefineHK");
    strTitle = BTWindowErrorDefineHK(con.TopPage()).strErrorTitle[int(titleID)];
    strMsg = ErrorMsg;
    BTWindowErrorDefineHK(con.TopPage()).SetData(strTitle, 5, strMsg);
    Log("[BTWindowErrorDefineHK::ShowError] ErrorMsg=" $ ErrorMsg);
    //return;    
}

